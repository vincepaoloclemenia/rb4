$('.flash-notice').replaceWith("<%= j render partial: 'partials/flash' %>");
retriggerFlash();

<% if @non_misce.is_sub_non_misce? %>
	$('#non_misce_<%= @non_misce.parent_id %>').find("#sub_non_misces").fadeOut();
<% else %>
	$('#non_misce_<%= @non_misce.id %>').fadeOut();
<% end %>


<% if current_client.non_misces.empty? %>
	$('.empty-warning').css("display","block");
	$('#non_misces').css("display","none");
<% else %>
	$('.empty-warning').css("display","none");
	$('#non_misces').css("display","block");
<% end %>